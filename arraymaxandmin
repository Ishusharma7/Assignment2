<body>
    <button onclick="maxvalue()";>MAX</button>
    <button onclick="minvalue()";>MIN</button>
    <script>
                let a = ['1','23424','23423','32432','324','2343'];
        function maxvalue(){
            let b = Math.max(...a);
            console.log(b);
        }
        function minvalue(){
            let b = Math.min(...a);
            console.log(b);
        }
    </script>
</body>
</html>
